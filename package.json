{
  "name": "cs458-schedulingtool",
  "version": "0.0.1",
  "description": "Scheduling Tool to consolidate data from several departments across campus into a single tool",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "start": "node ./server/server.js",
    "devServe": "nodemon --watch server/index.js --watch server/api/dataHelper.js server/index.js",
    "client": "esbuild --bundle --minify --target=ES6 --outfile=public/bundle.js client/app.jsx",
    "devClient": "nodemon --watch client --exec ./node_modules/.bin/esbuild --bundle --sourcemap --target=ES6 --outfile=public/bundle.js client/app.jsx",
    "test": "jest --coverage",
    "testJest": "jest --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kirbitz/CS458-SchedulingTool.git"
  },
  "keywords": [
    "Scheduling",
    "Stout"
  ],
  "author": "Tyler Marefke, Adam Lucka, Augustus Knaack, Caleb Czeck, Garrett Preston, Jacob Gibbs, Karsten Roland, Yoonseo Park",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kirbitz/CS458-SchedulingTool/issues"
  },
  "homepage": "https://github.com/Kirbitz/CS458-SchedulingTool#readme",
  "devDependencies": {
    "esbuild": "^0.15.10",
    "eslint": "^8.24.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "express": "^4.18.1",
    "@types/jest": "^29.1.2",
    "@types/supertest": "^2.0.12",
    "jest": "^29.1.2",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "mssql": "^9.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
